{
  "name": "oqc-test-andy",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently -k -r -s first \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "dev:web": "vite",
    "electron": "electron .",
    "build:web": "vite build --mode web",
    "build:dir": "vite build && electron-builder -w dir --publish never",
    "build:zip": "vite build && electron-builder -w zip --publish never",
    "build:win": "vite build && electron-builder -w nsis --publish never"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^10.0.0",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tailwindcss": "^4.1.13",
    "vite": "^7.1.2",
    "wait-on": "^8.0.4"
  },
  "build": {
    "appId": "com.example.oqc",
    "productName": "OQC Test",
    "asar": true,
    "files": [
      "dist/**",
      "electron/**",
      "package.json"
    ],
    "win": {
      "target": ["nsis", "zip"]
    },
    "directories": {
      "buildResources": "build"
    }
  }
}
